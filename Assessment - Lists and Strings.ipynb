    "ls = ['run', 'world', 'travel', 'lights', 'moon', 'baseball', 'sea']\n",
    "new = ls[2:4]\n",
    "print(new)\n",
    "\n",
    "A. ['travel', 'lights', 'moon']\n",
    "B. ['world', 'travel', 'lights']\n",
    "C. ['travel', 'lights']\n",
    "D. ['world', 'travel']\n",
    "\n",
    "Ans - C\n",
    "\n",
